<Application x:Class="Avrora.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Avrora"
             xmlns:settings="clr-namespace:Avrora.ViewModel.ViewModelSettings"
             xmlns:chatsettings="clr-namespace:Avrora.ViewModel.ViewModelChat"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/DarkWindowsStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <settings:ViewModelSettingsApplicationComponent x:Key="viewModelSettingsApplicationComponent"/>
            <settings:ViewModelUserSettingsComponent x:Key="viewModelUserSettingsComponent"/>
            <chatsettings:ViewModelChatsSettings x:Key="viewModelChatSettings"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
